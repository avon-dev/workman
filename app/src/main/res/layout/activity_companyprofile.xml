<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CompanyprofileActivity">

    <TextView
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:text="회사 프로필"
        app:layout_constraintBottom_toBottomOf="@+id/view" android:id="@+id/textView3"/>

    <ImageButton
        android:background="@drawable/ic_chevron_left_black_24dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="55dp"
        android:layout_height="55dp"/>

    <View
        android:layout_marginTop="55dp"
        android:background="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="1dp" android:id="@+id/view"/>

    <TextView
        android:id="@+id/CP_name"
        android:text="회사명"
        android:textStyle="bold"
        android:textSize="30dp"
        android:layout_margin="20dp"
        android:maxLines="1"
        android:ellipsize="end"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/CP_adresstext"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CP_name"
        android:text="주소"
        android:textSize="25dp"
        android:layout_width="100dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/CP_adress"
        android:background="@drawable/bottomline"
        android:maxLines="2"
        android:text="서울시 관악구 봉천동 1882-399"
        android:textSize="20dp"
        android:ellipsize="end"
        app:layout_constraintTop_toTopOf="@id/CP_adresstext"
        app:layout_constraintBottom_toBottomOf="@id/CP_adresstext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/CP_adresstext"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="20dp" />

    <TextView
        android:id="@+id/CP_phonetext"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="@id/CP_adresstext"
        app:layout_constraintTop_toBottomOf="@+id/CP_adress"
        android:text="대표번호"
        android:textSize="25dp"
        android:layout_width="100dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/CP_phone"
        android:background="@drawable/bottomline"
        android:maxLines="1"
        android:text="010-1234-5678"
        android:textSize="25dp"
        android:ellipsize="end"
        app:layout_constraintTop_toTopOf="@id/CP_phonetext"
        app:layout_constraintBottom_toBottomOf="@id/CP_phonetext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/CP_phonetext"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginRight="20dp" />

    <TextView
        android:id="@+id/CP_employeetext"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="@id/CP_phonetext"
        app:layout_constraintTop_toBottomOf="@+id/CP_phone"
        android:text="직원 수"
        android:textSize="25dp"
        android:layout_width="100dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/CP_employee"
        android:background="@drawable/bottomline"
        android:maxLines="1"
        android:text="직원"
        android:textSize="25dp"
        android:ellipsize="end"
        app:layout_constraintTop_toTopOf="@id/CP_employeetext"
        app:layout_constraintBottom_toBottomOf="@id/CP_employeetext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/CP_employeetext"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginRight="20dp" />

    <TextView
        android:id="@+id/CP_createdatetext"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="@id/CP_employeetext"
        app:layout_constraintTop_toBottomOf="@+id/CP_employee"
        android:text="생성 일"
        android:textSize="25dp"
        android:layout_width="100dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/CP_createdate"
        android:background="@drawable/bottomline"
        android:maxLines="1"
        android:text="0000년 00월 00일"
        android:textSize="25dp"
        android:ellipsize="end"
        app:layout_constraintTop_toTopOf="@id/CP_createdatetext"
        app:layout_constraintBottom_toBottomOf="@id/CP_createdatetext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/CP_createdatetext"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginRight="20dp" />

    <View
        android:layout_marginTop="30dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/CP_createdatetext"
        android:layout_width="match_parent"
        android:layout_height="0.5dp" android:id="@+id/view2"/>

</androidx.constraintlayout.widget.ConstraintLayout>